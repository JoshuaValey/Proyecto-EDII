
@model List<string>

@{
    ViewBag.Title = "BuscarMensaje";
}

<!DOCTYPE html>
<html>
<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-2">
            </div>
            <div class="col-md-8">
                <br />
                <br />
                <br />
                <br />
                <div class="page-header">
                    <h1>
                        Mensajes encontrados
                    </h1>
                </div>
                <br />
                <br />
                <table class="table-bordered">
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item)
                            </td>
                        </tr>
                    }
                </table>
            </div>
            <div class="col-md-2">
            </div>
        </div>
    </div>
</body>
</html>